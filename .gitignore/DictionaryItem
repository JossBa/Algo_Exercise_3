package de.htw.app;

import javax.xml.bind.annotation.XmlRootElement;

/**
 * A class for holds some information (Java based -> XML).
 * 
 * <ul>
 * <li>name
 * <li>date
 * </ul>
 * <p>
 * 
 * @author Erik Mautsch
 *
 */

@XmlRootElement
public class DictionaryItem {

	private String value;
	private int ranking;

	/**
	 * @return the value
	 */
	public String getValue() {
		return value;
	}

	/**
	 * @param value
	 *            the value to set
	 */
	public void setValue(String value) {
		this.value = value;
	}

	/**
	 * @return the ranking
	 */
	public int getRanking() {
		return ranking;
	}

	/**
	 * @param ranking
	 *            the ranking to set
	 */
	public void setRanking(int ranking) {
		this.ranking = ranking;
	}

}
